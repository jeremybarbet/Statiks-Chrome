{
  "name": "Statiks",
  "version": "1.0.0",
  "description": "Get the stats of your favorites networks.",
  "author": "Jérémy Barbet",
  "homepage_url": "http://www.jeremybarbet.com",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "http://api.dribbble.com/*",
    "https://twitter.com/*",
    "https://www.behance.net/v2/users/*",
    "https://api.500px.com/v1/*",
    "https://api.github.com/users/*",
    "http://vimeo.com/api/v2/*",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "38": "icon38.png"
    },
    "default_popup": "popup.html",
    "default_title": "Statiks"
  },
  "manifest_version": 2
}
